{
  "name": "ibelar/atk-wordpress",
  "type": "package",
  "description": "Enable use of agile toolkit framework in wordpress",
  "homepage": "https://github.com/ibelar/atk-wordpress",
  "license": "MIT",
  "keywords": ["WordPress","WordPress Plugin", "Agile Toolkit", "Atk4", "Atk4ui"],
  "authors": [
    {
      "name": "Alain Belair",
      "email": "belair.alain@gmail.com",
      "homepage": "https://github.com/ibelar"
    }
  ],
  "require": {
    "atk4/ui": "2.0.4",
    "atk4/data": "2.0.4",
    "atk4/core": "2.0.2",
    "atk4/dsql": "2.0.2"
  },
  "require-dev": {
    "phpunit/phpunit": "4.0.*"
  },
  "autoload": {
    "psr-4": {
      "atkwp\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "atkwp\\":"tests/"
    }
  }
}
